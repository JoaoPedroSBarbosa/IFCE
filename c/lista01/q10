#include <stdio.h>

int main (){

    float c, l, a;

    puts("Digite o valor do comprimento da caixa: ");
    scanf("%f",&c);

    puts("Digite o valor da largura da caixa: ");
    scanf("%f",&l);

    puts("Digite o valor da altura da caixa: ");
    scanf("%f",&a);

    printf("O volume da caixa é: %.2f m³", c*l*a);
  
    return 0;
}