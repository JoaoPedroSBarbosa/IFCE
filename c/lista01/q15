#include <stdio.h>

int main (){

    int d;

    puts("Quantos dias trabalhados? ");
    scanf("%d",&d);

    if(d<=10){
      printf("O salário será: %.2f", (50.25*d)*0.9);
    } else if(d < 20){
      printf("O salário será: %.2f", (50.25*d*1.2)*0.9);
    }else{
      printf("O salário será: %.2f", (50.25*d*1.3)*0.9);
    }
  
    return 0;
}