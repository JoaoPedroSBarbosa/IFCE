#include <stdio.h>

int main(){
  
  int a;
  int b;
  
  puts("Qual o valor do A?");
  scanf("%d",&a);

  puts("Qual o valor do B?");
  scanf("%d",&b);

  switch (a % b)
  {
  case 0:
    printf("O número %d é múltiplo de %d.", a, b);
    break;
  default:
    printf("O número %d não é múltiplo de %d.", a, b);
    break;
  }

  return 0;
}