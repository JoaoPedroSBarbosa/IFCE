#include <stdio.h>

int main(){
  
  printf("O número equivalente ao Símbolo A: %d", 'A');
  printf("\nO número equivalente ao Símbolo B: %d", 'B');
  printf("\nO número equivalente ao Símbolo C: %d", 'C');
  printf("\nO número equivalente ao Símbolo a: %d", 'a');
  printf("\nO número equivalente ao Símbolo b: %d", 'b');
  printf("\nO número equivalente ao Símbolo c: %d", 'c');
  printf("\nO número equivalente ao Símbolo 0: %d", '0');
  printf("\nO número equivalente ao Símbolo 1: %d", '1');
  printf("\nO número equivalente ao Símbolo 2: %d", '2');
  printf("\nO número equivalente ao Símbolo $: %d", '$');
  printf("\nO número equivalente ao Símbolo *: %d", '*');
  printf("\nO número equivalente ao Símbolo +: %d", '+');
  printf("\nO número equivalente ao Símbolo /: %d", '/');
  printf("\nO número equivalente ao espaço em branco: %d", ' ');
  
  return 0;
  
}